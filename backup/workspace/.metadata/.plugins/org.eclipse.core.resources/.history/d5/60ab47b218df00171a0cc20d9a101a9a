package test1;

public class Ifkiso {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ
		int a=10;
		if(a<20){
			System.out.println("A");
		}
		
		int b=20;
		if(b<=30){
			System.out.println("B");
		}
		
		String c="山田";
		if(c.equals("山田")){
			System.out.println(c+"さん");
		}
		
		int a1=5;
		if(a1<10 && 3<a1){
			System.out.println("A");
		}
		
		int a2=5;
		if(a2>=5 || a2<=0){
			System.out.println("B");
		}
		
		int a3=7;
		if(a3>8){
			System.out.println("A");
		}else if(a3<10){
			System.out.println("B");
		}

	}

}
