package test1;

public class Forkiso {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ
		
		for(int i=1; i<=10; i++){
			if(i%2==0){
				System.out.println(i+"\n");
			}
		}
		
		for(int a=0; a<3; a++){
			for(int b=1; b<4; b++){
				System.out.println(b);
			}
			System.out.println("\n");
		}
	}

}
